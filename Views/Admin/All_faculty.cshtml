            <div class="container w-100">

                <h4>All Faculty</h4>
                <hr />

                <table class="table">
                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.Fname)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Timing)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Education)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Skills)
                            </th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Fname)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Timing)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Education)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Skills)
                                </td>
                                <td>
                                    <a asp-action="Edit" asp-route-id="@item.FacultyId">Edit</a> |
                                    <a asp-action="Details" asp-route-id="@item.FacultyId">Details</a> |
                                    <a asp-action="Delete" asp-route-id="@item.FacultyId">Delete</a>
                                </td>
                            </tr>
                        }
                    </tbody>
